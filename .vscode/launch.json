{
    "configurations": [
    {
        "name": "ts-node",
        "type": "node",
        "request": "launch",
        "args": [
            "${relativeFile}"
        ],
        "runtimeArgs": [
            "-r",
            "ts-node/register"
        ],
        "cwd": "${workspaceRoot}",
        "protocol": "inspector",
        "internalConsoleOptions": "openOnSessionStart"
    },
    {
        "command": "npm run dev",
        "name": "Nuxt dev",
        "request": "launch",
        "type": "node-terminal"
    }
    ]
}